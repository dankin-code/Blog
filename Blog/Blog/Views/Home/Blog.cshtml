@*@model Inumerable<Blog.Models.Post>*@
@model Blog.Models.Post




@{
    ViewBag.Title = "Blog";
}

<h2>@ViewBag.Title.</h2>

<div>

    @Html.DisplayFor(model => model.MediaUrl)
    @Html.DisplayFor(model => model.PostTitle)
    <hr />
    @Html.DisplayFor(model => model.PostContent)
    <br />
    <pre>@Html.DisplayFor(model => model.PostCreationDate)</pre>
    <br />
    <br />

</div>
